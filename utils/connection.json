{
    "name": "my-network",
    "version": "1.0.0",
    "client": {
        "organization": "DepartmentIT",
        "connection": {
            "timeout": {
                "peer": {
                    "endorsing": "300",
                    "ordering": "300",
                    "eventHub": "300",
                    "discovery": "300"
                }
            }
        }
    },
    "organizations": {
        "DepartmentIT": {
            "mspid": "DepartmentITMSP",
            "peers": ["peer0.departmentIT.example.com"],
            "certificateAuthorities": ["ca.departmentIT.example.com"]
        },
        "DepartmentFinance": {
            "mspid": "DepartmentFinanceMSP",
            "peers": ["peer0.departmentFinance.example.com"],
            "certificateAuthorities": ["ca.departmentFinance.example.com"]
        },
        "DepartmentLogistics": {
            "mspid": "DepartmentLogisticsMSP",
            "peers": ["peer0.departmentLogistics.example.com"],
            "certificateAuthorities": ["ca.departmentLogistics.example.com"]
        }
    },
    "peers": {
        "peer0.departmentIT.example.com": {
            "url": "grpcs://localhost:7051",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.departmentIT.example.com",
                "grpc.keepalive_time_ms": 60000
            }
        },
        "peer0.departmentFinance.example.com": {
            "url": "grpcs://localhost:8051",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.departmentFinance.example.com",
                "grpc.keepalive_time_ms": 60000
            }
        },
        "peer0.departmentLogistics.example.com": {
            "url": "grpcs://localhost:9051",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.departmentLogistics.example.com",
                "grpc.keepalive_time_ms": 60000
            }
        }
    },
    "certificateAuthorities": {
        "ca.departmentIT.example.com": {
            "url": "https://localhost:7054",
            "caName": "ca-departmentIT",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            }
        },
        "ca.departmentFinance.example.com": {
            "url": "https://localhost:8054",
            "caName": "ca-departmentFinance",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            }
        },
        "ca.departmentLogistics.example.com": {
            "url": "https://localhost:9054",
            "caName": "ca-departmentLogistics",
            "tlsCACerts": {
                "path": "/path/to/tls/ca.crt"
            }
        }
    }
}
